table {
  width: 100%;
  border: 1px solid rgba(0, 0, 0, 0.05);

}



tr.example-detail-row {
  height: 0;
}

tr.example-element-row3:not(.example-expanded-row3):hover {
  background: #d9e2e2;
}

tr.example-element-row3:not(.example-expanded-row3):active {
  background: #efefef;
}

tr.example-element-row2:not(.example-expanded-row2):hover {
  background: #d9e2e2;
}

tr.example-element-row2:not(.example-expanded-row2):active {
  background: #efefef;
}

tr.example-element-row:not(.example-expanded-row):hover {
  background: #d9e2e2;
}

tr.example-element-row:not(.example-expanded-row):active {
  background: #efefef;
}

.example-element-row td {
  border-bottom-width: 0;
}

.example-element-detail {
  overflow: hidden;
  display: flex;
}

.example-element-diagram {
  min-width: 80px;
  border: 2px solid black;
  padding: 8px;
  font-weight: lighter;
  margin: 8px 0;
  height: 104px;
}

.example-element-symbol {
  font-weight: bold;
  font-size: 40px;
  line-height: normal;
}

.example-element-description {
  padding: 16px;
}

.example-element-description-attribution {
  opacity: 0.5;
}

.inner-table {
  width: 100%;
}

.inner-table mat-form-field {
  margin-top: 15px;
  width: 100%;
}

